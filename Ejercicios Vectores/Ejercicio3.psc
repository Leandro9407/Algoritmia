Algoritmo Ejercicio3
	
	Dimension a[5]
	
	Para i<-0 Hasta 4 Con Paso 1 Hacer
		Escribir "Ingrese número del vector: ", i
		leer a[4]
		
		Para f<-1 Hasta a[0] Con Paso 1 Hacer
			
			f=1
			
			multiplicador=a[i]*f
			factorial=multiplicador*f
			
			f=f+1
			
			Escribir "Factorial: ", factorial
		Fin Para
		
		
	Fin Para
	
	
	
	
FinAlgoritmo
